{
    // C/C++
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.debugShortcut": true,
    "C_Cpp.clang_format_path": "/opt/homebrew/opt/llvm/bin/clang-format",
    "C_Cpp.vcFormat.wrap.preserveBlocks": "never",
    "C_Cpp.vcFormat.space.withinSquareBrackets": true,
    "C_Cpp.clang_format_style": "WebKit",
    // cSpell
    "cSpell.userWords": [
        "antiderivative",
